<?xml version="1.0" encoding="iso-8859-1"?>
<root version="2.0">
	<template name="identitylist_entry">
		<genericcontrol>
			<anchored>
				<left>
					<parent>anchor</parent>
					<anchor>right</anchor>
					<relation>relative</relation>
					<offset>3</offset>
				</left>
				<top>
					<parent>anchor</parent>
					<anchor>top</anchor>
					<offset>2</offset>
				</top>
				<size>
					<width>70</width>
					<height>15</height>
				</size>
			</anchored>
			<frame>
				<name>mini_name</name>
			</frame>
			<script file="scripts/identitylist_entry.lua" />
		</genericcontrol>
	</template>

	<template name="characterlist_entry">
		<genericcontrol>
			<anchored>
				<left>
					<parent>anchor</parent>
					<anchor>right</anchor>
					<relation>relative</relation>
					<offset>3</offset>
				</left>
				<top>
					<parent>anchor</parent>
					<anchor>top</anchor>
				</top>
				<size>
					<width>75</width>
					<height>75</height>
				</size>
			</anchored>
			<script file="scripts/characterlist_entry.lua" />
		</genericcontrol>
	</template>
</root>
